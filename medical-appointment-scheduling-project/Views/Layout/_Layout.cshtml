          crossorigin="anonymous"
          referrerpolicy="no-referrer" />
    <link href="~/asset/style/main.min.css" rel="stylesheet" />
    @RenderSection("Styles", required: false)
</head>
<body>
    <div id="header">
        @Html.Partial("~/Views/Layout/_Header.cshtml")
