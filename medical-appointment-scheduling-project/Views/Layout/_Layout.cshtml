    <script src="~/asset/vender/js/cdn.jsdelivr.net_npm_popperjs_core2.10.2_dist_umd_popper.min.js"></script>
    <script src="~/asset/vender/js/cdn.jsdelivr.net_npm_bootstrap5.1.3_dist_js_bootstrap.min.js"></script>
