
@{
    ViewBag.Title = " Gói xét nghiệm tại nhà";
    Layout = "~/Views/Layout/_Layout.cshtml";
}

@section Styles {
    <link href="~/asset/style/page/post.min.css" rel="stylesheet" />
    <link href="~/asset/css/cssgroup.css" rel="stylesheet" />
}



<div class="container-lg py-4">
    <div class="row">
        <nav style="--bs-breadcrumb-divider: '»';" aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="#">Trang chủ</a></li>
                <li class="breadcrumb-item"><a href="#">Giới thiệu</a></li>
                <li class="breadcrumb-item active"> Gói xét nghiệm tại nhà</li>
            </ol>
        </nav>
    </div>
    <div class="row">
        <div class="col-md-9">
            <article class="blog-post">
                <h2 class="blog-post__title">
                    Gói xét nghiệm tại nhà
                </h2>
                <p class="mt-3 mb-3">
                    <strong>
                        Các gói khám sức khỏe tại bệnh viện Thu Cúc
                    </strong>
                </p>
                <div class="blog-post__content">
                    <div class="row">
                        <div class="card p-0" style="width: 22rem; margin-right: calc(3*24px/2); margin-bottom: 32px;">
                            <img src="~/asset/Service/1.jpg" class="card-img-top w-100" />
                            <div class="card-body">
                                <h5 class="card-title">Gói xét nghiệm sức khỏe tổng quát</h5>
                            </div>
                        </div>
                        <div class="card p-0" style="width: 22rem; margin-right: calc(3*24px/2); margin-bottom: 32px;">
                            <img src="~/asset/Service/2.jpg" class="card-img-top w-100" />
                            <div class="card-body">
                                <h5 class="card-title">Gói xét nghiệm sức khỏe tổng quát</h5>
                            </div>
                        </div>
                    </div>
                </div>
            </article>
        </div>

        <div class="col-md-3">
            <div class="position-sticky" style="top: 2rem;">
                <div class="widget-question">
                    <h3>Câu hỏi mới nhất</h3>
                    <hr />
                    <div class="widget-question__container">
                        <ul class="widget-question__list">
                            <li class="widget-question__item"><h3><a href="" class="widget-question__link" target="_blank">Làm sao để giải độc cơ thể?</a></h3></li>
                            <li class="widget-question__item"><h3><a href="" class="widget-question__link" target="_blank">Những triệu chứng phổ biến của ung thư phổi giai đoạn đầu</a></h3></li>
                            <li class="widget-question__item"><h3><a href="" class="widget-question__link" target="_blank">Các dịch vụ sẽ nhận được khi khám sức khỏe doanh nghiệp tại Thu Cúc TCI</a></h3></li>
                            <li class="widget-question__item"><h3><a href="" class="widget-question__link" target="_blank">Rối loạn tiền đình có dễ đột quỵ?</a></h3></li>
                            <li class="widget-question__item"><h3><a href="" class="widget-question__link" target="_blank">Người mắc đái tháo đường nên ăn những thực phẩm gì?</a></h3></li>
                        </ul>
                    </div>
                </div>
